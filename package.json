{
  "name": "react-cource",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "ts-node --esm server.ts",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "vi:test": "vitest run --coverage",
    "lint": "eslint . --fix",
    "cy:open": "cypress open --e2e --browser chrome",
    "cy:run": "cypress run",
    "cy:report": "npx nyc report --reporter=text-summary",
    "test:clean": "npx rimraf coverage && npx rimraf .nyc_output",
    "cy:e2e": "start-server-and-test start http-get://localhost:8000 cy:run",
    "cy:coverage-file": "npm run test:clean && start-server-and-test start http-get://localhost:8000 cy:open",
    "e2e:coverage": "npm run cy:e2e && npm run cy:report"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-redux": "^7.1.25",
    "express": "^4.18.2",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.8",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/express": "^4.17.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "cypress": "^12.10.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^7.0.4",
    "jsdom": "^21.1.1",
    "jsdom-worker": "^0.3.0",
    "lint-staged": "^12.4.1",
    "prettier": "2.8.4",
    "rimraf": "^5.0.0",
    "start-server-and-test": "^2.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-plugin-istanbul": "^4.0.1",
    "vitest": "^0.29.3"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,md}": "prettier --write",
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  }
}
